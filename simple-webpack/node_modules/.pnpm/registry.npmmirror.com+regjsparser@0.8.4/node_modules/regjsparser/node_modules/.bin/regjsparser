#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules/.pnpm/registry.npmmirror.com+regjsparser@0.8.4/node_modules/regjsparser/bin/node_modules:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules/.pnpm/registry.npmmirror.com+regjsparser@0.8.4/node_modules/regjsparser/node_modules:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules/.pnpm/registry.npmmirror.com+regjsparser@0.8.4/node_modules:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules/.pnpm/node_modules:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules:/Users/zxd/Documents/03-code/webpack-source/node_modules:/Users/zxd/Documents/03-code/node_modules:/Users/zxd/Documents/node_modules:/Users/zxd/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules/.pnpm/registry.npmmirror.com+regjsparser@0.8.4/node_modules/regjsparser/bin/node_modules:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules/.pnpm/registry.npmmirror.com+regjsparser@0.8.4/node_modules/regjsparser/node_modules:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules/.pnpm/registry.npmmirror.com+regjsparser@0.8.4/node_modules:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules/.pnpm/node_modules:/Users/zxd/Documents/03-code/webpack-source/simple-webpack/node_modules:/Users/zxd/Documents/03-code/webpack-source/node_modules:/Users/zxd/Documents/03-code/node_modules:/Users/zxd/Documents/node_modules:/Users/zxd/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/parser" "$@"
else
  exec node  "$basedir/../../bin/parser" "$@"
fi
